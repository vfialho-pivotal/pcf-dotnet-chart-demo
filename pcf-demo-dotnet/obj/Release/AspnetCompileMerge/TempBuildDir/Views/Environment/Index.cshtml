@model pcf_demo.Models.EnvironmentModel

@{
    ViewBag.Title = "Environment";
}

<h2>Application Instance Details</h2>

<div class="row">
    <div class="col-md-4">
        @Html.ActionLink("Show all", "Index", "Environment", new { all = "true" }, null)
        <table>
            @foreach (var vcapVar in Model.VcapVariables)
            {
                <tr><td>@(vcapVar.Key)</td><td>@(vcapVar.Value)</td></tr>
            }
        </table>
    </div>
</div>
